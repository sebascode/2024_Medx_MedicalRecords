{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "path:default",
    "event:default",
    "window:default",
    "app:default",
    "image:default",
    "resources:default",
    "menu:default",
    "tray:default",
    "shell:allow-open",
    "sql:allow-load",
    "sql:allow-close",
    "sql:allow-select",
    "sql:allow-execute",
    "dialog:allow-ask",
    "dialog:allow-save",
    "dialog:allow-open",
    "dialog:allow-confirm",
    "dialog:allow-message"
  ]
}
